{
    "name": "branogoga/rose-php",
    "description": "Rose PHP Library",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Brano Goga",
        "email": "brano.goga@centrum.sk"
    }],
    "readme": "./README.md",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1.0",
        "dibi/dibi": "~4",
        "nette/nette": "~3"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "phpstan/phpstan": "~1.10.26",
        "phpstan/phpstan-dibi": "~1.0.1",
        "phpstan/extension-installer": "~1.3.1",
        "phpstan/phpstan-nette": "~1.2.9",
        "phpstan/phpstan-deprecation-rules": "~1.1.3",
        "phpstan/phpstan-strict-rules": "^1.5.1"
    },
    "autoload": {
        "classmap": ["src"]
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
