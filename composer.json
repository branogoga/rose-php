{
    "name": "branogoga/rose-php",
    "description": "Rose PHP Library",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Brano Goga",
        "email": "brano.goga@centrum.sk"
    }],
    "readme": "./README.md",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "dibi/dibi": "~4",
        "nette/nette": "~3"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "phpstan/phpstan": ">0.11",
        "phpstan/phpstan-dibi": ">0.11",
        "phpstan/extension-installer": ">1.0",
        "phpstan/phpstan-nette": ">0.11"
    },
    "autoload": {
        "classmap": ["src"]
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}